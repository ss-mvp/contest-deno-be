{
  "name": "contest-be",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "https://github.com/story-squad/contest-deno-be.git",
  "author": "Brandon Ramirez <bran.ramirez.don@gmail.com>",
  "license": "MIT",
  "scripts": {
    "up": "docker-compose -f docker/docker-compose.yml up -d",
    "latest": "yarn knex migrate:latest",
    "latest:test": "yarn cross-env NODE_ENV=testing knex migrate:latest",
    "latest:ci": "yarn cross-env NODE_ENV=ci knex migrate:latest",
    "latest:prod": "yarn cross-env NODE_ENV=production knex migrate:latest",
    "latest:ds": "yarn cross-env NODE_ENV=ds knex migrate:latest",
    "rollback": "yarn knex migrate:rollback",
    "rollback:test": "yarn cross-env NODE_ENV=testing knex migrate:rollback",
    "rollback:ci": "yarn cross-env NODE_ENV=ci knex migrate:rollback",
    "rollback:prod": "yarn cross-env NODE_ENV=production knex migrate:rollback",
    "rollback:ds": "yarn cross-env NODE_ENV=ds knex migrate:rollback",
    "seed": "yarn knex seed:run",
    "seed:test": "yarn cross-env NODE_ENV=testing knex seed:run",
    "seed:ci": "yarn cross-env NODE_ENV=ci knex seed:run",
    "seed:prod": "yarn cross-env NODE_ENV=production knex seed:run",
    "seed:ds": "yarn cross-env NODE_ENV=ds knex seed:run",
    "seed:1": "yarn knex seed:run --specific=00_beforeAdminFlow.js",
    "reset": "yarn rollback && yarn latest && yarn seed",
    "reset:test": "yarn rollback:test && yarn latest:test",
    "reset:ci": "yarn rollback:ci && yarn latest:ci",
    "reset:ds": "yarn rollback:ds && yarn latest:ds",
    "reset:all": "yarn reset && yarn reset:test && yarn reset:ci",
    "make": "yarn knex migrate:make",
    "make:ds": "yarn cross-env NODE_ENV=ds knex migrate:make",
    "makeseed": "yarn knex seed:make",
    "makeseed:ds": "yarn cross-env NODE_ENV=ds knex seed:make",
    "lint:check": "npx eslint ."
  },
  "dependencies": {
    "aws-sdk": "^2.936.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.15",
    "knex-cleaner": "^1.3.1",
    "luxon": "^1.27.0",
    "pg": "^8.5.1",
    "typescript": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "2.2.1"
  }
}
